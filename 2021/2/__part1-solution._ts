const parseInput = (raw: string) => {
	const rows = raw
		.split('\n')
		.map(n => n.trim())
		.filter((v) => !!v)
		.map(v => {
			
			const [t, d] = v.split(' ');
			console.log(d);
			
			return {t, d: Number(d)}
		});


	return rows;
}

export const solve = (raw: string): any => {
	const input = parseInput(raw);

	const depth = input.reduce((a, cur) => {
		console.log(cur);
		
		if (cur.t === 'up') {
			return a - cur.d
		} else if (cur.t === 'down') {
			return a + cur.d;
		}
		return a;
	}, 0);

	const hor = input.reduce((a, cur) => {
		if (cur.t == 'forward') {
			return a + cur.d;
		}
		return a;
	},0 )


	return depth * hor;
};

